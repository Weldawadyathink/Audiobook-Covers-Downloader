{
  "imports": {
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@^3.826.0",
    "@aws-sdk/lib-storage": "npm:@aws-sdk/lib-storage@^3.828.0",
    "@keawade/zod-env": "jsr:@keawade/zod-env@^0.1.2",
    "@std/fs": "jsr:@std/fs@^1.0.18",
